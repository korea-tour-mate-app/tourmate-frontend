{
  "expo": {
    "name": "tourmate",
    "slug": "tourmate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/seoul_tower.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lehiewl.tourmate"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "web": {
        "bundler": "metro",
        "output": "static",
        "favicon": "./assets/images/favicon.png"
      },
      "plugins": [
        "expo-router"
      ],
      "experiments": {
        "typedRoutes": true
      }
    },
    "extra": {
      "eas": {
        "projectId": "96d12d73-2170-4263-b8ff-e93b3257923c"
      }
    }
  }
}
